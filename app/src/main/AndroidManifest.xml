<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.serma.crocobisunes.timetable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".FullscreenImg"></activity>
        <activity
            android:name=".ChoosenTimetable"
            android:screenOrientation="landscape" />
        <activity
            android:name=".myTimetable"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Happiness"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Achievement"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TimeTable"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Communicator"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Calendar"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>